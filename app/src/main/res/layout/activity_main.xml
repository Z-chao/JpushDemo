<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.z_chao.jpushdemo.MainActivity">

    <Button
        android:id="@+id/bt_aaa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我是管理员, 我要推送给用户"/>

    <Button
        android:id="@+id/bt_bbb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我是用户,我要推送给用户的用户"/>
    <Button
        android:id="@+id/bt_ccc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我是用户"/>

    <Button
        android:id="@+id/bt_ddd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我是用户的用户"/>
</LinearLayout>
